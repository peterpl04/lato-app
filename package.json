{
  "name": "latoapps",
  "version": "1.1.3",
  "description": "Centro de aplicações Lato",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "electron": "^29.0.0",
    "electron-builder": "^24.13.3",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "build": {
    "appId": "com.lato.latoapps.v3",
    "productName": "LatoApps",
    "artifactName": "LatoApps.Setup.${version}.${ext}",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icons/lato-infinite.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "peterpl04",
        "repo": "lato-app"
      }
    ]
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "express": "^5.2.1",
    "pg": "^8.17.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3"
  }
}
